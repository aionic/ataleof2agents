# Foundry Agent Environment Variables
# Copy this file to .env and fill in your values

# Azure AI Foundry Project Configuration (required)
AZURE_AI_PROJECT_ENDPOINT=https://your-project.cognitiveservices.azure.com/
AZURE_AI_MODEL_DEPLOYMENT_NAME=gpt-4

# Weather Function Configuration (required)
WEATHER_FUNCTION_URL=https://your-function-app.azurewebsites.net/api/get_weather

# Azure Application Insights (optional - Foundry provides native telemetry)
APPLICATIONINSIGHTS_CONNECTION_STRING=InstrumentationKey=your-key-here
